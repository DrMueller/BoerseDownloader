<UserControl x:Class="MMU.BoerseDownloader.WpfUI.UserControls.DownloadEntriesFilterUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:MMU.BoerseDownloader.WpfUI.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <GroupBox Header="Filtering" HorizontalContentAlignment="Center">
        <CheckBox IsChecked="{Binding ShowVisitedDownloadEntries}" Content="Show Visited" />
    </GroupBox>
</UserControl>